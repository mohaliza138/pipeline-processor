    subi R4, R4, 1
    lw R4, R2, 0
    lw R4, R2, 0
    add R3, R2, R3
    subi R4, R4, 1

    lw R4, R7, 0
    lw R4, R7, 0
    ret:
    jr R7 

end:
sb R4, R3, 0
